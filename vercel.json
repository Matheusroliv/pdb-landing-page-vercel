{
  "installCommand": "yarn install --ignore-platform",
  "buildCommand": "yarn run build",

  "outputDirectory": "dist/portal-landing-page/browser",

  "routes": [
    { "handle": "filesystem" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/[^./]+(?:/[^./]+)*", "dest": "/index.html" }
  ]
}

{
  "installCommand": "yarn install --ignore-platform",
  "buildCommand": "yarn run build",
  "outputDirectory": "dist/portal-landing-page",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/",
      "dest": "/browser/index.html"
    },
    {
      "src": "/[^./]+(?:/[^./]+)*",
      "dest": "/browser/index.html"
    }
  ]
}